# prevent directory listing
Options -Indexes
IndexIgnore */*

# follow symbolic links
Options FollowSymlinks
RewriteEngine on
# without trailing slash
RewriteRule ^administrator(/.+)?$ backend/web/$1 [L,PT]
# with trailing slash
RewriteRule ^administrator/(.+)?$ backend/web/$1 [L,PT]
RewriteRule ^(.+)?$ frontend/web/$1 [L,PT]

